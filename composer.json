{
    "name": "mammatus/http-server-websockets",
    "description": "\ud83d\udd78\ufe0f\ud83e\udde6 WebSockets for HTTP Server",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "^7.4",
        "ancarda/psr7-string-stream": "^1.3",
        "chimera/foundation": "^0.3.0",
        "chimera/mapping": "^0.3.0",
        "league/tactician": "^1.0",
        "mammatus/http-server-contracts": "dev-master",
        "react-parallel/worker-pool": "^1.0",
        "ringcentral/psr7": "^1.3",
        "thecodingmachine/safe": "^1.0"
    },
    "require-dev": {
        "wyrihaximus/async-test-utilities": "3.4.12"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mammatus\\Http\\Server\\WebSockets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mammatus\\Tests\\Http\\Server\\WebSockets\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
